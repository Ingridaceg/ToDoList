{
	"name": "todolist",
	"version": "1.0.0",
	"description": "my own todo list",
	"main": "index.js",
	"scripts": {
		"start": "react-scripts start",
		"server": "set NODE_ENV=development && node server/index.js",
		"lint": "eslint"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.json": [
			"prettier --write"
		],
		"*.{js,jsx}": [
			"eslint --cache --fix",
			"prettier --write"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Ingridaceg/ToDoList.git"
	},
	"keywords": [
		"to",
		"do",
		"list",
		"react"
	],
	"author": "Ingrid Acero",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Ingridaceg/ToDoList/issues"
	},
	"dependencies": {
		"axios": "^0.20.0",
		"body-parser": "^1.19.0",
		"bootstrap": "^4.5.2",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"lodash": "^4.17.20",
		"mongoose": "^5.10.3",
		"prop-types": "^15.7.2",
		"react": "^16.13.1",
		"react-bootstrap": "^1.3.0",
		"react-dom": "^16.13.1",
		"react-scripts": "^3.4.3",
		"uid": "^1.0.0"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"babel-eslint": "^10.1.0",
		"babel-plugin-root-import": "^6.5.0",
		"eslint": "^7.11.0",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-config-prettier": "^6.12.0",
		"eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-react": "^7.21.4",
		"eslint-plugin-react-hooks": "^4.1.2",
		"husky": "^4.3.0",
		"lint-staged": "^10.4.0",
		"prettier": "^2.1.2"
	}
}
